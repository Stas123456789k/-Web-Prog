
        //Заданеи 1 (Реализуйте функцию, которая принимает два числа m и n и возвращает массив
        //первых m кратных вещественного числа n. Предположим, m — это положительное целое число.)
using System;  

public static class Kata  
{  
    public static double[] Multiples(int m, double n)  
    {  
        double[] result = new double[m]; 
        for (int i = 1; i <= m; i++)  
        {  
            result[i - 1] = i * n;  
        }  
        return result;   
    }  
}
        Заданеи 2 (//Ваша задача — написать функцию, которая принимает строку
        //и возвращает новую строку без гласных.)
        using System;  
using System.Linq;  

public static class Kata  
{  
    public static string Disemvowel(string input)  
    {   
        string vowels = "aeiouAEIOU"; 
        var result = new string(input.Where(c => !vowels.Contains(c)).ToArray());  
        return result;  
    }  
} 
    
